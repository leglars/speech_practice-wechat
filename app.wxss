.cell {
  border-top: 1rpx solid #e5e5e5;
  padding: 24rpx 56rpx;
  position: relative;
  display: block;
}

.cell:first-child {
  border-top: 0;
}

.cell:after {
  clear: both;
  display: table;
  content: " ";
}

.cell-label {
  font-size: 34rpx;
  color: #343434;
  letter-spacing: 0;
}

.cell-label-highlight {
  font-size: 34rpx;
  color: #000;
}

.highlight {
  color: #576B95;
  font-weight: 500;
}

.left {
  float: left;
}

.right {
  float: right;
}

.copyright {
  /* Copyright @ 2017 合肥学: */
  position: absolute;
  bottom: 12rpx;
  font-size: 20rpx;
  color: #B2B2B2;
}