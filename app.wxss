@import './pages/common/common.wxss';

.container {
  display: flex;
  flex-direction: column;
  height:100%;
  min-height: 1335rpx;
  align-items: center;
  box-sizing: border-box;
  background-color: #EFEFF4;
} 

.cardContainer {
  margin-top: 20rpx;
  background-color: #fff;
  width: 100%;
  position: relative;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
}

